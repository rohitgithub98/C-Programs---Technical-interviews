#include<stdio.h>
int main() {
	int low,high,a[50],i=0,no=0,r,j=0,b[50];
	printf("Enter lower value :");
	scanf("%d",&low);
	printf("Enter higher value :");
	scanf("%d",&high);
	while(low<high)
	{
		j=0;
		if(low<10)
		{
			a[i]=low;
			++i;
		}
		
		else
		{
			while(low>0)
			{
				r=low%10;
				b[j]=r;
				++j;
				low=low/10;
			}
			
			while(j>=0)
			{
				a[i]=b[j];
				++i;
				--j;
			}
		}
		
		++low;
	}
	
	printf("\nEnter the desired index : ");
	scanf("%d",&no);
	printf("\n%d",a[no]);
}
